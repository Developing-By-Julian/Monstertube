<!DOCTYPE html>
<html>
<head>
    <title>Leaderboard</title>
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Lexend:wght@400;500&display=swap">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">    <style>
        body {
            background-color: #212529;
            color: #fff;
            font-family: 'Lexend', sans-serif;
            margin: 0;
            padding: 0;
        }
    </style>
</head>
<body>
    <div class="container">
        <h1>Leaderboard</h1>
        <ul>
            <% data.users.forEach(userObj => { %>
                <%= console.log(userObj) %>
                <li>
                    <span class="username"><%= userObj.user_fetch.username %></span> - 
                    <span class="money"> Cash: <%= userObj.user.cash %> Bank: <%= userObj.user.bank %></span>
                </li>
            <% }); %>
        </ul>
        <a href="/dashboard" class="btn btn-primary">Dashboard</a>
    </div>
</body>
</html>
